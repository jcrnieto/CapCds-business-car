{
  "name": "school",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8.7.2",
    "@sap/xssec": "^4.4.0",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.8.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "credentials": {
            "url": "db.sqlite"
          }
        },
        "[production]": {
          "kind": "hana",
          "db": "hana",
          "auth": "xsuaa"
        }
      },
      "deploy": {
        "autoUndeploy": false
      }
    },
    "build": {
      "target": "gen"
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}
